pipeline{
       agent any
    
       tools {
             maven 'maven'
             jdk   'java-11'
            }
       stages {
        stage('git-checkout'){
           steps {
               git branch:'version-1',url:'https://github.com/ManojKRISHNAPPA/microdegree-IT-batch-2025.git'
                 }
              }
        stage('compile'){
           steps {
                  sh "mvn compile"
                 }
               }
        stage('Build') {
          steps {
                 sh "mvn package"
                }
             }
          }
}
